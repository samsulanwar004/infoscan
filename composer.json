{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "predis/predis": "^1.1",
        "rebel/component-util": "dev-master",
        "rebel/component-rbac": "dev-master",
        "rebel/component-setting": "dev-master",
        "league/flysystem-aws-s3-v3": "^1.0",
        "barryvdh/laravel-cors": "^0.8.2",
        "tymon/jwt-auth": "dev-develop",
        "barryvdh/laravel-debugbar": "^2.3",
        "spatie/laravel-fractal": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://pieter-rebel-works@bitbucket.org/rebelworksco/rebel-component-utilities.git"
        },
        {
            "type": "git",
            "url": "https://pieter-rebel-works@bitbucket.org/rebelworksco/rebel-component-rbac.git"
        },
        {
            "type": "git",
            "url": "https://pieter-rebel-works@bitbucket.org/rebelworksco/rebel-component-setting.git"
        },
        {
            "type": "composer",
            "url": "https://packagist.org"
        },
        {
            "packagist": false
        }
    ],
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
